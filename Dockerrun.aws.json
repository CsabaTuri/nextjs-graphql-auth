{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "db",
      "image": "mongo:latest",
      "hostname": "db",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 27017,
          "containerPort": 27017
        }
      ]
    },
    {
      "name": "client",
      "image": "nreoch9/nextjs-graphql-auth-client:latest",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "graphql",
      "image": "nreoch9/nextjs-graphql-auth-server:latest",
      "hostname": "graphql",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "nreoch9/nextjs-graphql-auth-nginx:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "graphql"],
      "memory": 128
    }
  ]
}
